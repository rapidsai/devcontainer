{
  "build": {
    "dockerfile": "./Dockerfile",
    "context": "."
  },
  "remoteUser": "coder",
  "features": {
    "ghcr.io/devcontainers/features/common-utils:2": {
      "uid": "1000",
      "gid": "1000",
      "username": "coder",
      "installZsh": "true",
      "upgradePackages": "true"
    },
    "ghcr.io/devcontainers/features/git:1": {
      "ppa": "true"
    },
    "ghcr.io/devcontainers/features/git-lfs:1": {},
    "ghcr.io/devcontainers/features/github-cli:1": {},
    "./features/gitlab-cli": {},
    "./features/cmake": {},
    "./features/ninja": {},
    "./features/sccache": {
      "version": "0.4.0-pre.6"
    },
    "./features/llvm": {},
    "./features/cuda": {},
    "./features/utils": {}
  },
  "overrideFeatureInstallOrder": [
    "ghcr.io/devcontainers/features/common-utils",
    "ghcr.io/devcontainers/features/git",
    "ghcr.io/devcontainers/features/git-lfs",
    "ghcr.io/devcontainers/features/github-cli",
    "./features/gitlab-cli",
    "./features/cmake",
    "./features/ninja",
    "./features/sccache",
    "./features/llvm",
    "./features/cuda",
    "./features/utils"
  ],
  "updateContentCommand": [
    "bash",
    "-li",
    "/opt/devcontainer/bin/update-content-command.sh"
  ],
  "onCreateCommand": [
    "bash",
    "-li",
    "/opt/devcontainer/bin/on-create-command.sh"
  ],
  "postCreateCommand": [
    "bash",
    "-li",
    "/opt/devcontainer/bin/post-create-command.sh"
  ],
  "postStartCommand": [
    "bash",
    "-li",
    "/opt/devcontainer/bin/post-start-command.sh"
  ],
  "postAttachCommand": [
    "bash",
    "-li",
    "/opt/devcontainer/bin/post-attach-command.sh"
  ]
}
