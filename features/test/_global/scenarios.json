{
  "cuda_rust": {
    "image": "ubuntu:22.04",
    "features": {
      "rust": {},
      "cuda": {
        "version": "12.0"
      }
    },
    "overrideFeatureInstallOrder": [
      "./rust",
      "./cuda"
    ]
  },
  "cmake_cuda_gitlabcli_llvm_ninja_nvhpc_sccache": {
    "image": "ubuntu:22.04",
    "features": {
      "ghcr.io/devcontainers/features/git:1": {
        "ppa": "true"
      },
      "ghcr.io/devcontainers/features/git-lfs:1": {},
      "ghcr.io/devcontainers/features/github-cli:1": {},
      "gitlab-cli": {
        "version": "latest"
      },
      "cmake": {
        "version": "3.25.2"
      },
      "ninja": {
        "version": "1.11.1"
      },
      "sccache": {
        "version": "0.3.1"
      },
      "llvm": {
        "version": "15"
      },
      "cuda": {
        "version": "12.0"
      },
      "nvhpc": {
        "version": "23.1"
      }
    },
    "overrideFeatureInstallOrder": [
      "./gitlab-cli",
      "./cmake",
      "./ninja",
      "./sccache",
      "./llvm",
      "./cuda",
      "./nvhpc"
    ]
  }
}
